{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "lib-badges": [
        "dist/lib-badges/lib-badges",
        "dist/lib-badges"
      ],
      "lib-posts": [
        "dist/lib-posts/lib-posts",
        "dist/lib-posts"
      ],
      "braga-lib": [
        "dist/braga-lib/braga-lib",
        "dist/braga-lib"
      ]
    }
  }
}